{
    "name": "jasirilabs/swahiba-sms",
    "description": "Aggregating SMS services to a single API",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^0.2.1",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^11.1"
    },
    "autoload": {
        "psr-4": {
            "JasiriLabs\\SwahibaSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JasiriLabs\\SwahibaSms\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "authors": [
        {
            "name": "Bruno Alfred",
            "email": "mambo@brunoalfred.com"
        }
    ]
}
